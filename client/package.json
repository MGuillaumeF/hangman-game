    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "compression-webpack-plugin": "^9.0.1",
    "css-loader": "^5.2.5",
    "css-minimizer-webpack-plugin": "^3.1.4",
    "cypress": "^9.1.0",
    "cypress-movie": "^3.0.20",
