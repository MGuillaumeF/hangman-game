{
  "author": "MGuillaumeF",
  "name": "model-generator",
  "version": "0.1.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "build:dev": "webpack --define-process-env-node-env development --config ./webpack.config.mjs --progress --color",
    "build": "webpack --define-process-env-node-env production --config ./webpack.config.mjs --progress --color",
    "build:doc": "typedoc --out docs ./src/main.ts",
    "postbuild": "npm run start ../../resources/model.xml",
    "poststart": "prettier --write dist",
    "prettier": "prettier --write . --ignore-path node_modules",
    "start": "node build/main.bundle.js",
    "lint:eslint": "eslint . --fix --format html --output-file ./dist/reports/lint/eslint-report.html --ignore-path ./.gitignore",
    "lint:eslint:json": "eslint . --fix --format json --output-file ./dist/reports/lint/eslint-report.json --ignore-path ./.gitignore",
    "test": "npm-run-all -p start cy:run",
    "webpack-stats": "webpack --node-env production --profile --json=stats.json"
  },
  "dependencies": {
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-transform-regenerator": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "@gfx/zopfli": "^1.0.15",
    "@mguillaumef/audiso": "^0.0.4",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.3",
    "babel-plugin-polyfill-corejs3": "^0.9.0",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^11.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "eslint-webpack-plugin": "^4.0.1",
    "prettier": "^3.2.4",
    "ts-loader": "^9.5.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "typedoc": "^0.25.7",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-visualizer-plugin2": "^1.1.0"
  }
}
