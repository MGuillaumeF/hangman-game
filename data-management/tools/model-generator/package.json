{
  "author": "MGuillaumeF",
  "name": "model-generator",
  "version": "0.1.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "build:dev": "webpack --node-env development --config ./webpack.config.mjs --progress --color",
    "build": "webpack --node-env production --config ./webpack.config.mjs --progress --color",
    "postbuild": "npm run start ../../resources/model.xml",
    "poststart": "prettier --write dist",
    "prettier": "prettier --write . --ignore-path node_modules",
    "start": "node build/main.bundle.js",
    "lint:eslint": "eslint . --fix --format html --output-file ./dist/reports/lint/eslint-report.html --ignore-path ./.gitignore",
    "lint:eslint:json": "eslint . --fix --format json --output-file ./dist/reports/lint/eslint-report.json --ignore-path ./.gitignore",
    "test": "npm-run-all -p start cy:run",
    "webpack-stats": "webpack --node-env production --profile --json=stats.json"
  },
  "dependencies": {
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-transform-regenerator": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@gfx/zopfli": "^1.0.15",
    "@mguillaumef/audiso": "^0.0.4",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-polyfill-corejs3": "^0.6.0",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-webpack-plugin": "^3.2.0",
    "prettier": "^2.7.1",
    "ts-loader": "^9.4.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-visualizer-plugin2": "^1.0.0"
  }
}
