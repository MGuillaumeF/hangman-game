<? xml version="1.0" ?>
<mime-types>
    <mime-type ext=".css" value="text/css" allowed="true"/>
    <mime-type ext=".html" value="text/html" allowed="true"/>
    <mime-type ext=".htm" value="text/html" allowed="true"/>
    <mime-type ext=".php" value="text/html" allowed="true"/>
    <mime-type ext=".txt" value="text/plain" allowed="true"/>
