<TestSuite name="[HTTP][API] Test Token Endpoint">
  <Properties>
    <URL>${protocol}://${hostname}:${port}${prefix}/token</URL>
    <Headers>
      <Content-Type>application/json</Content-Type>
    </Headers>
    <Timeout>15000</Timeout>
  </Properties>
  <TestCases>
    <Case name="[GET] Test Token - method not allowed">
      <Request>
        <Methode>GET</Methode>
      </Request>
      <Response>
        <Status>405</Status>
      </Response>
    </Case>
    <Case name="[PUT] Test Token - method not allowed">
      <Request>
        <Methode>PUT</Methode>
      </Request>
      <Response>
        <Status>405</Status>
      </Response>
    </Case>
  </TestCases>
</TestSuite>
